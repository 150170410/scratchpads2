@article {14,
	title = {The genus Dugesia in Australia, with its phylogenetic analysis and historical biogeography (Platyhelminthes, Tricladida, Dugesiidae)},
	journal = {Zoologica Scripta},
	volume = {27},
	year = {1998},
	pages = {273-289},
	chapter = {273},
	abstract = {The new species Dugesia notogaea Sluys \& Kawakatsu, sp. n. is described from Queensland, Australia, representing the first record of the genus Dugesia for that continent. A phylogenetic analysis of the 68 species of Dugesia resulted in the discovery of a new apomorphy for the genus, and a cladogram showing two major phyletic groups. The distributional range of one group has a western accent, whereas that of the other group has an eastern emphasis; the ranges of these two groups overlap in Africa and the Mediterranean region. We attempt to determine the ancestral area for both the Dugesiidae and Dugesia. A scenario for the historical biogeography of the genus Dugesia is discussed. In this scenario it is presumed that the geological history of the Australasian region enabled Dugesia{\textquoteright}s from southeast Asia to colonize northern Australia.},
	doi = {10.1111/j.1463-6409.1998.tb00461.x},
