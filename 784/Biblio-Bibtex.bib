@article {755,
	title = {Report on the ecological survey of freshwater planarians in Kyoto city and its adjacent district, Honshu},
	journal = {Japanese Journal of Ecology},
	volume = {17},
	number = {3},
	year = {1967},
	pages = {112-118},
	abstract = {In the vicinity of Kyoto City, 4 species of freshwater planarians, Dugesia japonica, Phagocata vivida, Phagocata kawakatsui and Bdellocephala brunnea, were found. D. japonica was most common in the stations both in the plain and the mountainous districts (below the altitude of about 950 m). P. vivida was rather common in the cold-water stations within the altitude range from about 120 to 950 m or more. P. kawakatsui was found only in a spring-fed stream in Kameoka City (Asahi-cho; the type locality of this species) and in a shallow well at Saga-tenryuji-chd in Kyoto City. A small population of B. brunnea was found in a large spring-fed stream at Nishioji-sichijo in Kyoto City (the southernmost locality of this species). This station is well known to biologists as the locality of the sexual race of D. japonica (the Nishioji stock). The type of the vertical distribution in the area surveyed is shown as J-JV-V (J; D japonica; V: P vivida). The 2 species of true lake-dwelling tri-clads, BdellocepEala annandalci and Dendrocoelopsis? sp. of Lake Biwa-ko, were recorded from the muddy bottom (35 to 50 m or more in depth) of the middle and northern parts of Lake Biwa-ko.},
	author = {Masaharu Kawakatsu and Ogawara, Genchu and Tarui, Yoshitsugu},
	keywords = {Dugesia japonica Ichikawa & Kawakatsu, 1964, Phagocata kawakatsui Okugawa, 1956, Phagocata vivida (Ijima & Kaburaki, 1916), Bdellocephala brunnea Ijima & Kaburaki, 1916}
