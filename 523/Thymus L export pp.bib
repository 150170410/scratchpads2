number = {5},
pages = {791--795},
publisher = {Society of Chemical Industry},
title = {Bioactivities of essential oil and extract of Thymus argaeus, Turkish endemic wild thyme},
url = {http://dx.doi.org/10.1002/jsfa.3513 http://dx.doi.org/10.1002/(ISSN)1097-0010},
volume = {89},
