keywords = {Allelopathy,Antigerminative activity,Monoterpenes,Natural herbicides,Thymus L.,Thymus vulgaris,Volatile compounds},
mendeley-tags = {Thymus L.,Thymus vulgaris},
pages = {347--352},
title = {Allelopathic activity of essential oils from Mediterranean Labiatae},
