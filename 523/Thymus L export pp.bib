@article{Albert2002,
abstract = {The aim of this work was to study the seed germination characteristics, under different incubation temperatures and using different pretreatments for promoting germination, of nine species, Boleum asperum (Pers.) Desvaux, Centaurea pinnata Pau, Halopeplis amplexicaulis (Vahl.) Ung.-Sternb, Krascheninnikovia ceratoides (L.) Gueldenst, Lythrum flexuosum Lag., Microcnemum coralloides (Loscos \& Pardo) Buen, Pucinellia pungens (Pau) Paunero, Thymus loscosii Willk., and Vella pseudocytisus L. subsp. paui Gomez Campo, that grow in semiarid regions of NE Spain. In eight of these species, seeds collected from several (2-9) populations were tested. Constant 15 degreesC, 20 degreesC, or 25 degreesC, and alternating 15/25 degreesC temperature regimes and 16/8 hours light/dark photoperiod conditions were used. In seeds from the different populations of Halopeplis amplexicaulis and Microcnemum coralloides, manual scarification, dry heat, and soaking in gibberellic acid solution were used as pretreatments applied for enhancing germination. Germination variables recorded were final germination percentage and germination rate (T,,). Incubation temperatures had a significant effect on the final germination percentage in five species, while germination rate was differentially affected by temperature in only two species. Significant differences among populations under the same temperature were detected for both germination percentage and germination rate in most of the species studied. In general, both final germination percentage and germination rate of H. amplexicaulis and M. coralloides were increased by the pretreatments assayed. In these two species, the highest germination percentages were reached by manual scarification.},
annote = {PT: J},
author = {Albert, M J and Iriondo, J M and Perez-Garcia, F},
journal = {Israel Journal of Plant Sciences},
keywords = {Thymus L.,Thymus loscosii},
mendeley-tags = {Thymus L.,Thymus loscosii},
number = {2},
pages = {103--112},
title = {Effects of temperature and pretreatments on seed germination of nine semiarid species from NE Spain},
volume = {50},
year = {2002}
